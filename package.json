{
  "name": "@moznion/limited-concurrent-workers",
  "version": "0.0.1",
  "description": "A library that provides a limited number of concurrent workers system",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --no-cache --coverage",
    "lint": "eslint .",
    "fmt": "find -name '*.ts' -not -path '*.d.ts' -not -path './node_modules/*' | xargs prettier --write",
    "gendoc": "typedoc ./*.ts",
    "release": "npm run build && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moznion/limited-concurrent-workers-js.git"
  },
  "bugs": {
    "url": "https://github.com/moznion/limited-concurrent-workers-js/issues"
  },
  "homepage": "https://github.com/moznion/limited-concurrent-workers-js#readme",
  "author": {
    "name": "moznion",
    "email": "moznion@mail.moznion.net"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ]
}
